    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "RUN: collect-code",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/app/main.py",
            "console": "integratedTerminal",
            "args": [
                "olympic",
                "collect-code",
                "-d"
            ]
        },
        {
            "name": "RUN: collect-sport",
